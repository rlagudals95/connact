{
  "name": "connact",
  "version": "0.0.0",
  "type": "module",
  "description": "connact",
  "engines": {
    "node": ">=18.16.0"
  },
  "packageManager": "npm@9.5.1",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "watch": "vite build --watch --mode production",
    "watch:dev": "vite build --watch --mode development",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "test": "jest",
    "lint": "eslint --max-warnings=-1 \"src/**/**.ts*\"",
    "lint:fix": "npm run lint -s -- --fix",
    "prepare": "husky install && npm run setup-husky-hook:pre-commit && npm run setup-husky-hook:pre-push",
    "setup-husky-hook:pre-commit": "husky add .husky/pre-commit \"npm run lint && npx commitlint --edit\"",
    "setup-husky-hook:pre-push": "husky add .husky/pre-push \"npm run build\"",
    "semantic-release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jest/globals": "^29.3.1",
    "@types/chrome": "^0.0.180",
    "@types/jest": "^29.5.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^29.3.1",
    "eslint": "^8.13.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-enzyme": "^7.1.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5",
    "typestyle": "^2.3.0",
    "vite": "^2.9.1",
    "vite-plugin-babel": "^1.1.3",
    "vite-plugin-minify": "^1.5.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "semantic-release": "^21.0.3"
  }
}
